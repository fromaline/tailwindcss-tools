{
	"useTabs": true,
	"singleQuote": true,
	"semi": false,
	"trailingComma": "es5",
	"arrowParens": "avoid",
	"printWidth": 100,
	"plugins": [
		"prettier-plugin-svelte",
		"prettier-plugin-tailwindcss"
	],
	"overrides": [
		{
			"files": "*.svelte",
			"options": {
				"parser": "svelte",
				"plugins": [
					"prettier-plugin-svelte",
					"prettier-plugin-tailwindcss"
				]
			}
		}
	]
}